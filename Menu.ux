				<Grid RowCount="1" ColumnCount="9" Width="80%">
					<!--Panel Background="#d1d1f3" Width="50%" Height="90%" Y="15"-->
					<Circle Height="30" Y="20">
						<Text FontSize="20" TextColor="Gray" Value="1" TextAlignment="Center" Font="Bold" />
					</Circle>
					
					
				</Grid>

			</Panel>
			
		</StackPanel>
		
	</Panel>